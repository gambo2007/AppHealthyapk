<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomePage">

    <TextView
        android:id="@+id/nameshow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="150dp"
        android:layout_marginRight="150dp"
        android:text="TextView"
        android:textColor="#120101"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/man2"
                android:layout_width="201dp"
                android:layout_height="55dp"
                android:layout_marginStart="130dp"
                android:layout_marginLeft="173dp"
                android:layout_marginEnd="110dp"
                android:layout_marginRight="140dp"
                android:layout_marginBottom="34dp"
                android:layout_weight="1"
                android:text="Covid Statistics"
                app:backgroundTint="#300F77"
                app:layout_constraintBottom_toTopOf="@+id/logout"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/nameshow"
                app:layout_constraintVertical_bias="0.093" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/Profit"
                android:layout_width="224dp"
                android:layout_height="55dp"
                android:layout_marginStart="130dp"
                android:layout_marginLeft="173dp"
                android:layout_marginTop="1dp"
                android:layout_marginEnd="110dp"
                android:layout_marginRight="140dp"
                android:layout_marginBottom="34dp"
                android:layout_weight="1"
                android:text="ProFile"
                app:backgroundTint="#300F77"
                app:layout_constraintBottom_toTopOf="@+id/logout"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.333"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/nameshow"
                app:layout_constraintVertical_bias="0.104" />

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="82dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/RiskA"
                android:layout_width="wrap_content"
                android:layout_height="55dp"
                android:layout_marginStart="133dp"
                android:layout_marginLeft="173dp"
                android:layout_marginTop="3dp"
                android:layout_marginEnd="110dp"
                android:layout_marginRight="150dp"
                android:text="Risk Assessment"
                app:backgroundTint="#300F77"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.803"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Profit" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/Inf"
                android:layout_width="153dp"
                android:layout_height="54dp"
                android:layout_marginStart="133dp"
                android:layout_marginLeft="173dp"
                android:layout_marginTop="3dp"
                android:layout_marginEnd="120dp"
                android:layout_marginRight="150dp"
                android:text="Information About Covid"
                app:backgroundTint="#300F77"
                tools:layout_editor_absoluteX="129dp"
                tools:layout_editor_absoluteY="401dp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/Protect"
                android:layout_width="153dp"
                android:layout_height="54dp"
                android:layout_marginStart="135dp"
                android:layout_marginLeft="132dp"
                android:layout_marginTop="30dp"
                android:layout_marginEnd="105dp"
                android:layout_marginRight="150dp"
                android:layout_marginBottom="152dp"
                app:backgroundTint="#300F77"
                android:layout_weight="1"
                android:text="Advice" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/logout"
                android:layout_width="104dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="155dp"
                android:layout_marginLeft="123dp"
                android:layout_marginTop="50dp"
                android:layout_marginEnd="150dp"
                android:layout_marginRight="150dp"
                android:layout_marginBottom="152dp"
                android:text="Logout"
                android:textColor="#120101"
                app:backgroundTint="#DCD6D6"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent" />

        </LinearLayout>

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>